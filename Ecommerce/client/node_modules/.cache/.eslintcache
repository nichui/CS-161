e={title}\r\n                onChange={handleChange}\r\n            />\r\n        </div>\r\n\r\n        <div className=\"form-group\">\r\n            <label>\r\n                Description\r\n            </label>\r\n            <input\r\n                type=\"text\"\r\n                name=\"description\"\r\n                className=\"form-control\"\r\n                value={description}\r\n                onChange={handleChange}\r\n            />\r\n        </div>\r\n\r\n        <div className=\"form-group\">\r\n            <label>\r\n                Price\r\n            </label>\r\n            <input\r\n                type=\"number\"\r\n                name=\"price\"\r\n                className=\"form-control\"\r\n                value={price}\r\n                onChange={handleChange}\r\n            />\r\n        </div>\r\n\r\n        <div className=\"form-group\">\r\n            <label>\r\n                Shipping\r\n            </label>\r\n            <select name=\"shipping\" className=\"form-control\" onChange={handleChange}>\r\n                <option value=\"No\">Please select</option>\r\n                <option value=\"No\">No</option>\r\n                <option value=\"Yes\">Yes</option>\r\n\r\n            </select>\r\n        </div>\r\n\r\n        <div className=\"form-group\">\r\n            <label>\r\n                Quantity\r\n            </label>\r\n            <input\r\n                type=\"number\"\r\n                name=\"quantity\"\r\n                className=\"form-control\"\r\n                value={quantity}\r\n                onChange={handleChange}\r\n            />\r\n        </div>\r\n\r\n        <div className=\"form-group\">\r\n            <label>\r\n                Season\r\n            </label>\r\n            <select\r\n                name=\"season\"\r\n                className=\"form-control\"\r\n                onChange={handleChange}\r\n            >\r\n                <option> Please select </option>\r\n                {seasons.map((season) =>\r\n                    (<option key={season} value={season}>\r\n                        {season}\r\n                    </option>\r\n                    ))}\r\n\r\n            </select>\r\n        </div>\r\n\r\n        <div className=\"form-group\">\r\n            <label>\r\n                Brand\r\n            </label>\r\n            <select name=\"brand\" className=\"form-control\" onChange={handleChange}>\r\n                <option value=\"No\">Please select</option>\r\n                {brands.map(brand =>\r\n                    <option key={brand} value={brand}>\r\n                        { brand }\r\n                    </option>)}\r\n\r\n            </select>\r\n        </div>\r\n\r\n        <div className=\"form-group\">\r\n            <label>Category</label>\r\n            <select\r\n                name=\"category\"\r\n                className=\"form-control\"\r\n                onChange={handleCategoryChange}\r\n            >\r\n                <option>Please Select</option>\r\n                {categories.length > 0 &&\r\n                categories.map((category) => (<option key={category._id} value={category._id}>\r\n                    {category.name}\r\n                </option>))}\r\n            </select>\r\n        </div>\r\n        {/*{subOptions ? subOptions.length : 'no subs yet'}*/}\r\n\r\n        {/*{categories.length}*/}\r\n\r\n        {showSub && (\r\n            <div>\r\n            <label>Sub Categories</label>\r\n            <Select\r\n                mode = \"multiple\"\r\n                style={{width: '100%'}}\r\n                placeholder= \"Please Select\"\r\n                value={subs}\r\n                onChange={(value) => setValues({...values, subs: value})}\r\n            >\r\n\r\n                {subOptions.length &&\r\n                subOptions.map((s) => (\r\n                    <Option key={s._id} value={s._id}>\r\n                        {s.name}\r\n                    </Option>\r\n                ))}\r\n            </Select>\r\n        </div>\r\n        )}\r\n        <br/>\r\n        <div className=\"form-group\">\r\n            <label>Calendar</label>\r\n            <select\r\n                name=\"calendar\"\r\n                className=\"form-control\"\r\n                onChange={handleChange}\r\n            >\r\n                <option>Please select</option>\r\n                {calendarOptions.map(calOption =>\r\n                    <option key={calOption._id} value={calOption._id}>\r\n                        { calOption.name }\r\n                    </option>)}\r\n            </select>\r\n        </div>\r\n\r\n        <button className=\"btn btn-outline-info\">\r\n            Save\r\n        </button>\r\n\r\n    </form>\r\n);\r\n};\r\n\r\nexport default ProductCreateForm;","C:\\Users\\vn100\\Desktop\\CoolBeans\\CS-161\\Ecommerce\\client\\src\\components\\forms\\FileUpload.js",[],"C:\\Users\\vn100\\Desktop\\CoolBeans\\CS-161\\Ecommerce\\client\\src\\components\\forms\\ProductUpdateForm.js",["512","513","514"],"import React from 'react'\r\nimport ProductCreate from \"../../pages/admin/product/ProductCreate\";\r\nimport {Select} from 'antd'\r\nconst {Option} = Select;\r\n\r\n\r\nconst ProductUpdateForm = ({\r\n                               handleSubmit,\r\n                               handleChange,\r\n                               setValues,\r\n                               values,\r\n                               handleCategoryChange,\r\n                               categories,\r\n                               subOptions,\r\n                               arrayOfSubs,\r\n                               setArrayOfSubs,\r\n                               selectedCategory,\r\n                               calendarOptions\r\n                           }) => {\r\n    // destructure\r\n    const {\r\n        title,\r\n        description,\r\n        price,\r\n        //categories,\r\n        category,\r\n        subs,\r\n        shipping,\r\n        quantity,\r\n        images,\r\n        seasons,\r\n        brands,\r\n        season,\r\n        brand,\r\n        calendar,\r\n    } = values;\r\n    return (\r\n\r\n        <form onSubmit={handleSubmit}>\r\n            <div className=\"form-group\">\r\n                <label>\r\n                    Title\r\n                </label>\r\n                <input\r\n                    type=\"text\"\r\n                    name=\"title\"\r\n                    className=\"form-control\"\r\n                    value={title}\r\n                    onChange={handleChange}\r\n                />\r\n            </div>\r\n\r\n            <div className=\"form-group\">\r\n                <label>\r\n                    Description\r\n                </label>\r\n                <input\r\n                    type=\"text\"\r\n                    name=\"description\"\r\n                    className=\"form-control\"\r\n                    value={description}\r\n                    onChange={handleChange}\r\n                />\r\n            </div>\r\n\r\n            <div className=\"form-group\">\r\n                <label>\r\n                    Price\r\n                </label>\r\n                <input\r\n                    type=\"number\"\r\n                    name=\"price\"\r\n                    className=\"form-control\"\r\n                    value={price}\r\n                    onChange={handleChange}\r\n                />\r\n            </div>\r\n\r\n            <div className=\"form-group\">\r\n                <label>\r\n                    Shipping\r\n                </label>\r\n                <select\r\n                    value={shipping === 'Yes' ? 'Yes' : 'No'}\r\n                    name=\"shipping\" className=\"form-control\" onChange={handleChange}>\r\n                    {/*<option value=\"No\">Please select</option>*/}\r\n                    <option value=\"No\">No</option>\r\n                    <option value=\"Yes\">Yes</option>\r\n\r\n                </select>\r\n            </div>\r\n\r\n            <div className=\"form-group\">\r\n                <label>\r\n                    Quantity\r\n                </label>\r\n                <input\r\n                    type=\"number\"\r\n                    name=\"quantity\"\r\n                    className=\"form-control\"\r\n                    value={quantity}\r\n                    onChange={handleChange}\r\n                />\r\n            </div>\r\n\r\n            <div className=\"form-group\">\r\n                <label>\r\n                    Season\r\n                </label>\r\n                <select\r\n                    value={season}\r\n                    name=\"season\"\r\n                    className=\"form-control\"\r\n                    onChange={handleChange}\r\n                >\r\n\r\n                    {seasons.map((season) =>\r\n                        (<option key={season} value={season}>\r\n                                {season}\r\n                            </option>\r\n                        ))}\r\n\r\n                </select>\r\n            </div>\r\n\r\n            <div className=\"form-group\">\r\n                <label>\r\n                    Brand\r\n                </label>\r\n                <select value={brand} name=\"brand\" className=\"form-control\" onChange={handleChange}>\r\n\r\n                    {brands.map(brand =>\r\n                        <option key={brand} value={brand}>\r\n                            { brand }\r\n                        </option>)}\r\n\r\n                </select>\r\n            </div>\r\n\r\n            <div className=\"form-group\">\r\n                <label>Category</label>\r\n                <select\r\n                    name=\"category\"\r\n                    className=\"form-control\"\r\n                    onChange={handleCategoryChange}\r\n                    value={selectedCategory ? selectedCategory : category._id}\r\n                >\r\n                    {/*<option>{category ? category.name : 'Please select'}</option>*/}\r\n\r\n                    {categories.length > 0 &&\r\n                    categories.map((category) => (<option key={category._id} value={category._id}>\r\n                        {category.name}\r\n                    </option>))}\r\n                </select>\r\n            </div>\r\n\r\n            <div>\r\n                <label>Sub Categories</label>\r\n                <Select\r\n                    mode = \"multiple\"\r\n                    style={{width: '100%'}}\r\n                    placeholder= \"Please Select\"\r\n                    value={arrayOfSubs}\r\n                    onChange={(value) => setArrayOfSubs(value)}\r\n                >\r\n                    {subOptions.length &&\r\n                        subOptions.map((s) => (\r\n                        <Option key={s._id} value={s._id}>\r\n                            {s.name}\r\n                        </Option>\r\n                        ))}\r\n                </Select>\r\n            </div>\r\n\r\n\r\n\r\n            {/*{subOptions ? subOptions.length : 'no subs yet'}*/}\r\n\r\n            {/*{categories.length}*/}\r\n            <div className=\"form-group\">\r\n                <label>Calendar</label>\r\n                <select\r\n                    name=\"calendar\"\r\n                    className=\"form-control\"\r\n                    onChange={handleChange}\r\n                    value={calendar._id}\r\n                >\r\n                    <option>Please select</option>\r\n                    {calendarOptions.map(calOption =>\r\n                        <option key={calOption._id} value={calOption._id}>\r\n                            { calOption.name }\r\n                        </option>)}\r\n                </select>\r\n            </div>\r\n\r\n            <br/>\r\n\r\n            <button className=\"btn btn-outline-info\">\r\n                Save\r\n            </button>\r\n\r\n        </form>\r\n    );\r\n};\r\n\r\nexport default ProductUpdateForm;","C:\\Users\\vn100\\Desktop\\CoolBeans\\CS-161\\Ecommerce\\client\\src\\components\\forms\\CategoryForm.js",[],"C:\\Users\\vn100\\Desktop\\CoolBeans\\CS-161\\Ecommerce\\client\\src\\components\\cards\\ProductCardInCheckout.js",["515","516","517"],"import React from 'react';\r\nimport ModalImage from 'react-modal-image'\r\nimport Default from '../../images/Default.jpg';\r\nimport {useDispatch} from \"react-redux\";\r\nimport { ToastContainer, toast } from 'react-toastify';\r\nimport {CheckCircleOutlined, CloseCircleOutlined, CloseOutlined} from \"@ant-design/icons\";\r\n\r\n\r\nconst amOrPm = (time) => {\r\n    return parseInt(time) > 12 ? \"PM\" : \"AM\";\r\n}\r\nconst simpleHourFormat = (time) => {\r\n    const startTime = (parseInt(time[0].split(':')[0]) + 11) % 12 + 1;\r\n    const endTime = (parseInt(time[1].split(':')[0]) + 11) % 12 + 1;\r\n    return `${startTime}:${time[0].split(':')[1]} ${amOrPm(time[0])} - ${endTime}:${time[1].split(':')[1]} ${amOrPm(time[1])}`;\r\n}\r\n\r\nconst ProductCardInCheckout = ({p}) => {\r\n    let dispatch = useDispatch();\r\n    // const seasons = ['Spring', 'Summer', 'Fall', 'Winter', 'Whole Year'];\r\n    // const handleSeasonChange = e => {\r\n    //     console.log('Season changed', e.target.value);\r\n\r\n    //     let cart = []\r\n    //     if(typeof window !== 'undefined'){\r\n    //         if(localStorage.getItem('cart')){\r\n    //             cart = JSON.parse(localStorage.getItem('cart'));\r\n    //         }\r\n\r\n    //         cart.map((product, i) => {\r\n    //             if(product._id === p._id && isSameReservation(product.reservation, p.reservation)){\r\n    //                 cart[i].season = e.target.value;\r\n    //             }\r\n    //         });\r\n\r\n    //         //console.log('card update season', cart)\r\n    //         localStorage.setItem(\"cart\", JSON.stringify(cart));\r\n    //         dispatch({\r\n    //             type: \"ADD_TO_CART\",\r\n    //             payload: cart,\r\n    //         })\r\n    //     }\r\n    // };\r\n\r\n    const handleQuantityChange = e => {\r\n        //console.log('available quantity', p.quantity);\r\n\r\n        let count = e.target.value < 1 ? 1 : e.target.value;\r\n        if(count > p.quantity){\r\n            toast.error(`Max available quantity: ${p.quantity}`);\r\n            return;\r\n        }\r\n\r\n        let cart = []\r\n        if(typeof window !== 'undefined'){\r\n            if(localStorage.getItem('cart')){\r\n                cart = JSON.parse(localStorage.getItem('cart'));\r\n            }\r\n            cart.map((product, i) => {\r\n                if(product._id === p._id && isSameReservation(product.reservation, p.reservation)){\r\n                    cart[i].count = count;\r\n                }\r\n            });\r\n\r\n            localStorage.setItem('cart', JSON.stringify(cart));\r\n            dispatch({\r\n                type: \"ADD_TO_CART\",\r\n                payload: cart,\r\n            })\r\n        }\r\n    }\r\n    const isSameReservation = (res1, res2) => {\r\n        return res1.selectedDate === res2.selectedDate && \r\n                res1.timeRange[0] === res2.timeRange[0] && \r\n                res1.timeRange[1] === res2.timeRange[1]\r\n    }\r\n\r\n    const handleRemove = () => {\r\n        //console.log(p._id, \"to Remove\")\r\n        let cart = []\r\n        if(typeof window !== 'undefined'){\r\n            if(localStorage.getItem('cart')){\r\n                cart = JSON.parse(localStorage.getItem('cart'));\r\n            }\r\n            // [1,2,3,4,5]\r\n            cart.map((product, i) => {\r\n                if(product._id === p._id && isSameReservation(product.reservation, p.reservation)){\r\n                    cart.splice(i, 1);\r\n                }\r\n            });\r\n\r\n            localStorage.setItem('cart', JSON.stringify(cart));\r\n            dispatch({\r\n                type: \"ADD_TO_CART\",\r\n                payload: cart,\r\n            })\r\n        }\r\n    }\r\n    return(\r\n        <tbody>\r\n            <tr>\r\n                <td>\r\n                    <div style={{width: \"100px\", height: \"auto\"}}>\r\n                        {p.images && p.images[0] ? (\r\n                            <ModalImage\r\n                                small={p.images[0].url}\r\n                                large={p.images[0].url}\r\n                            />\r\n                            ) : (\r\n                            <ModalImage\r\n                                small={Default}\r\n                                large={Default}\r\n                            />\r\n                        )}\r\n                    </div>\r\n                </td>\r\n                <td>{p.title}</td>\r\n                <td>$ {p.price}</td>\r\n                <td>{p.brand}</td>\r\n                <td>{p.reservation.selectedDate}<br></br>{simpleHourFormat(p.reservation.timeRange)}</td>\r\n                {/* <td>\r\n                    <select\r\n                        onChange={handleSeasonChange}\r\n                        name=\"season\"\r\n                        className=\"form-control\"\r\n                        id=\"\">\r\n                        {p.season ? <option value={p.season}>\r\n                            {p.season}\r\n                        </option> : <option>\r\n                            Select\r\n                        </option>}\r\n                        {seasons.filter((s) => s !== p.season).map((s) => <option key={s} value={s}>{s}</option>)}\r\n                    </select>\r\n                </td> */}\r\n                <td className=\"text-center\">\r\n                    <input\r\n                        type=\"number\"\r\n                        className=\"form-control\"\r\n                        value={p.count}\r\n                        onChange={handleQuantityChange}\r\n                    />\r\n                </td>\r\n                <td className=\"text-center\">{\r\n                    p.shipping === \"Yes\" ?\r\n                        <CheckCircleOutlined\r\n                            className=\"text-success text-center\"/> :\r\n                        <CloseCircleOutlined\r\n                            className=\"text-danger text-center\"\r\n                        />\r\n                }</td>\r\n                <td className=\"text-center\"><CloseOutlined\r\n                    onClick={handleRemove}\r\n                    className=\"text-danger pointer\"\r\n                /></td>\r\n            </tr>\r\n        </tbody>\r\n    )\r\n}\r\n\r\nexport default ProductCardInCheckout","C:\\Users\\vn100\\Desktop\\CoolBeans\\CS-161\\Ecommerce\\client\\src\\components\\order\\Invoice.js",[],"C:\\Users\\vn100\\Desktop\\CoolBeans\\CS-161\\Ecommerce\\client\\src\\components\\order\\Orders.js",[],"C:\\Users\\vn100\\Desktop\\CoolBeans\\CS-161\\Ecommerce\\client\\src\\components\\cards\\ShowPaymentInfo.js",[],{"ruleId":"518","severity":1,"message":"519","line":1,"column":8,"nodeType":"520","messageId":"521","endLine":1,"endColumn":12},{"ruleId":"518","severity":1,"message":"522","line":45,"column":10,"nodeType":"520","messageId":"521","endLine":45,"endColumn":24},{"ruleId":"518","severity":1,"message":"523","line":45,"column":26,"nodeType":"520","messageId":"521","endLine":45,"endColumn":30},{"ruleId":"518","severity":1,"message":"524","line":46,"column":10,"nodeType":"520","messageId":"521","endLine":46,"endColumn":32},{"ruleId":"518","severity":1,"message":"525","line":54,"column":9,"nodeType":"520","messageId":"521","endLine":54,"endColumn":27},{"ruleId":"518","severity":1,"message":"526","line":114,"column":20,"nodeType":"520","messageId":"521","endLine":114,"endColumn":29},{"ruleId":"518","severity":1,"message":"527","line":2,"column":10,"nodeType":"520","messageId":"521","endLine":2,"endColumn":16},{"ruleId":"518","severity":1,"message":"528","line":2,"column":18,"nodeType":"520","messageId":"521","endLine":2,"endColumn":30},{"r
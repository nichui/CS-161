vious state.\r\n\r\n        this.setState((prevState) => ({\r\n            ...prevState,\r\n            messages: [...prevState.messages, message],\r\n        }));\r\n    }\r\n}\r\n\r\nexport default ActionProvider;","C:\\Users\\vn100\\Desktop\\CoolBeans\\CS-161\\Ecommerce\\client\\src\\chatbot\\MessageParser.js",[],"C:\\Users\\vn100\\Desktop\\CoolBeans\\CS-161\\Ecommerce\\client\\src\\pages\\About.js",["397","398","399","400","401","402","403","404","405","406","407"],"import React, {useState, useEffect} from 'react'\r\n\r\nimport { Avatar } from 'antd';\r\nimport { UserOutlined } from '@ant-design/icons';\r\nimport { Menu, Slider, Checkbox } from 'antd'\r\nimport { Image } from 'antd';\r\nimport { Container, Row, Col } from \"reactstrap\";\r\nimport { Card, ListGroup, ListGroupItem } from 'reactstrap';\r\n\r\nconst {SubMenu, ItemGroup} = Menu;\r\n\r\nconst About = () => {\r\n\r\n    return (\r\n        <>\r\n        <link\r\n            href=\"https://fonts.googleapis.com/css?family=Montserrat:400,700,200\"\r\n            rel=\"stylesheet\"\r\n            />\r\n        <link\r\n            href=\"https://maxcdn.bootstrapcdn.com/font-awesome/latest/css/font-awesome.min.css\"\r\n            rel=\"stylesheet\"\r\n            />\r\n            <h1 className=\"text-center\">\r\n                About Us\r\n            </h1>\r\n\r\n            <h2 className=\"text-center mb-1 py-3\">The Team</h2>\r\n            \r\n            <div id=\"images\">\r\n          <Container>\r\n            <Row>\r\n            <Col className=\"mr-auto ml-auto\" md=\"2\" sm=\"2\">\r\n                <h4 className>Sarah</h4>\r\n                <img\r\n                  alt=\"...\"\r\n                  className=\"img-circle img-no-padding img-responsive\"\r\n                  src={require(\"../images/sarah.png\").default}\r\n                />\r\n                <p className=\"text-center\">Team Lead<br></br>Front End Developer</p>\r\n              </Col>\r\n              <Col className=\"mr-a
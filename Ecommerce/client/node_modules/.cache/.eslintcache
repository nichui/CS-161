\r\n                </button> {/*if user didn't enter email then button will be disabled*/}\r\n            </form>\r\n    </div>\r\n    );\r\n}\r\n\r\nexport default ForgotPassword;\r\n\r\n\r\n\r\n\r\n\r\n\r\n\r\n",{"ruleId":"69","replacedBy":"70"},{"ruleId":"71","replacedBy":"72"},{"ruleId":"73","severity":1,"message":"74","line":1,"column":8,"nodeType":"75","messageId":"76","endLine":1,"endColumn":12},{"ruleId":"77","severity":1,"message":"78","line":55,"column":8,"nodeType":"79","endLine":55,"endColumn":10,"suggestions":"80"},{"ruleId":"73","severity":1,"message":"81","line":5,"column":10,"nodeType":"75","messageId":"76","endLine":5,"endColumn":22},{"ruleId":"73","severity":1,"message":"82","line":6,"column":5,"nodeType":"75","messageId":"76","endLine":6,"endColumn":20},{"ruleId":"73","severity":1,"message":"83","line":7,"column":5,"nodeType":"75","messageId":"76","endLine":7,"endColumn":17},{"ruleId":"73","severity":1,"message":"84","line":8,"column":5,"nodeType":"75","messageId":"76","endLine":8,"endColumn":20},{"ruleId":"73","severity":1,"message":"85","line":9,"column":5,"nodeType":"75","messageId":"76","endLine":9,"endColumn":19},{"ruleId":"77","severity":1,"message":"86","line":27,"column":8,"nodeType":"79","endLine":27,"endColumn":14,"suggestions":"87"},{"ruleId":"77","severity":1,"message":"86","line":15,"column":8,"nodeType":"79","endLine":15,"endColumn":14,"suggestions":"88"},{"ruleId":"73","severity":1,"message":"89","line":4,"column":8,"nodeType":"75","messageId":"76","endLine":4,"endColumn":12},{"ruleId":"73","severity":1,"message":"90","line":3,"column":16,"nodeType":"75","messageId":"76","endLine":3,"endColumn":30},{"ruleId":"77","severity":1,"message":"86","line":18,"column":8,"nodeType":"79","endLine":18,"endColumn":14,"suggestions":"91"},"no-native-reassign",["92"],"no-negated-in-lhs",["93"],"no-unused-vars","'logo' is defined but never used.","Identifier","unusedVar","react-hooks/exhaustive-deps","React Hook useEffect has a missing dependency: 
andleSeason = (e) => {\r\n        setSub('')\r\n        dispatch({\r\n            type: \"SEARCH_QUERY\",\r\n            payload: {text: \"\"},\r\n        })\r\n        setPrice([0,0]);\r\n        setCategoryIds([]);\r\n        setStar('');\r\n        setBrand('');\r\n        setSeason(e.target.value);\r\n        setShipping('');\r\n        fetchProducts({ season: e.target.value});\r\n    };\r\n\r\n    // 9. show products based on shipping yes/no\r\n    const showShipping = () => (\r\n        <>\r\n            <Checkbox\r\n                className=\"pt-2 pb-2 pl-4 pr-4\"\r\n                onChange={handleShippingChange}\r\n                value=\"Yes\"\r\n                checked={shipping === \"Yes\"}\r\n            >\r\n                Yes\r\n            </Checkbox>\r\n\r\n            <Checkbox\r\n                className=\"pt-1 pb-2 pl-3 pr-4\"\r\n                onChange={handleShippingChange}\r\n                value=\"No\"\r\n                checked={shipping === \"No\"}\r\n            >\r\n                No\r\n            </Checkbox>\r\n        </>\r\n    )\r\n\r\n    const handleShippingChange = (e) => {\r\n        setSub('')\r\n        dispatch({\r\n            type: \"SEARCH_QUERY\",\r\n            payload: {text: \"\"},\r\n        })\r\n        setPrice([0,0]);\r\n        setCategoryIds([]);\r\n        setStar('');\r\n        setBrand('');\r\n        setSeason('');\r\n        setShipping(e.target.value);\r\n        fetchProducts({ shipping: e.target.value});\r\n    }\r\n\r\n\r\n\r\n    return (\r\n        <div className=\"container-fluid\">\r\n            <div className=\"row\">\r\n                <div className=\"col-md-3 pt-2\">\r\n                    <h4>Search/Filter</h4>\r\n                    <hr/>\r\n                    <Menu defaultOpenKeys={['1', '2', '3' , '4', '5', '6', '7']} mode=\"inline\">\r\n                        {/* PRICE*/}\r\n                        <SubMenu key=\"1\" title={\r\n                            <span className=\"h6\">\r\n                            <DollarOutlined/> Price\r\n                        </span>\r\n                        }\r\n                        >\r\n                            <div>\r\n                                <Slider\r\n                                    className=\"ml-4 mr-4\"\r\n                                    tipFormatter={(v) => `$${v}`}\r\n                                    range\r\n                                    value={price}\r\n                                    onChange={handleSlider}\r\n                                    max=\"4999\"\r\n                                />\r\n                            </div>\r\n                        </SubMenu>\r\n\r\n\r\n                        {/*CATEGORY*/}\r\n                        <SubMenu\r\n                            key=\"2\"\r\n                            title={\r\n                                <span className=\"h6 text-warning\">\r\n                                    <DownSquareOutlined/> Categories\r\n                                </span>\r\n                            }\r\n                        >\r\n                            <div style={{ marginTop: \"-10px\"}}>\r\n                                {showCategories()}\r\n                            </div>\r\n                        </SubMenu>\r\n\r\n                        {/* STARS */}\r\n                        <SubMenu\r\n                            key=\"3\"\r\n                            title={\r\n                                <span className=\"h6 text-warning\">\r\n                                    <StarOutlined /> Rating\r\n                                </span>\r\n                            }\r\n                        >\r\n                            <div style={{ marginTop: \"-10px\"}}>\r\n                                {showStars()}\r\n                            </div>\r\n                        </SubMenu>\r\n\r\n                        {/*SUB CATEGORIES*/}\r\n\r\n                        <SubMenu\r\n           